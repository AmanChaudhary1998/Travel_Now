<%include adminheader%>

<!--Main Section Starts-->

<div id="tooplate_main">

    <center>

        <h3 style="color: chocolate;"><%=msg%></h3>
        <h2 style="padding-top:20px; padding-bottom:20px">Add Sub Category</h2>
    <form method="post" enctype="multipart/form-data" action="" style="padding-top:20px; padding-bottom:20px" >
      <table cellspacing="20">
      <tr>
      <td><label>Category Name</label></td>
      <td>
        <select name="catnm" style="width: 66%;">
          <option value="">Select Category</option>
          <%for(let row of clist) {%>
            <option><%= row.catnm%></option>
            <%}%>
        </select>
      </td>
      </tr>
      <tr>
      <td><label>Sub Category Name</label></td>
      <td><input type="text" name="subcatnm" placeholder="Enter Category Name"/></td>
      </tr>
      <tr>
      <td><label>Category Icon</label></td>
      <td><input type="file" name="subcaticon" /></td>
      </tr>
      <tr>
      <td><input type="submit" value="Add Sub Category" /></td>
      </tr>
      </table>
      </form>
    </center>




    <div class="cleaner"></div>
</div>

<div class="cleaner"></div>

<!--Main Section Ends-->

<%include footer%>
